<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="oval"
            android:useLevel="true" android:visible="true">
            <solid android:color="#00ECE5D8"/>
            <stroke android:color="#00A5A5A5" android:width="3dp"/><size
            android:width="52dp"
            android:height="52dp" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="oval"
            android:useLevel="true" android:visible="true" >
            <solid android:color="#00ECE5D8"/>
            <stroke android:color="#00A5A5A5" android:width="3dp"/><size
            android:width="52dp"
            android:height="52dp" />
        </shape>
    </item>
</selector>