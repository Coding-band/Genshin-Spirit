<animated-selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:id="@+id/on"
        android:state_checked="true"
        android:drawable="@drawable/ic_radio_on_2048" />
    <item
        android:id="@+id/off"
        android:drawable="@drawable/ic_radio_off_2048" />
    <transition
        android:fromId="@+id/on"
        android:toId="@+id/off"
        android:drawable="@drawable/btn_radio_on_to_off_mtrl_animation" />
    <transition
        android:fromId="@+id/off"
        android:toId="@+id/on"
        android:drawable="@drawable/btn_radio_off_to_on_mtrl_animation" />
</animated-selector>
